<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Byte217.MAUI.Application.Maui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:Byte217.MAUI.Application.Maui.Pages"
    xmlns:local="clr-namespace:Byte217.MAUI.Application.Maui"
    Shell.FlyoutBehavior="Disabled"
    Title="Byte217 MAUI Controls">

    <ShellContent
        ContentTemplate="{DataTemplate pages:MainPage}"
        Route="MainPage" />
    <ShellContent
        ContentTemplate="{DataTemplate pages:SettingsPage}"
        Route="SettingsPage" />
    <ShellContent
        ContentTemplate="{DataTemplate pages:TextPage}"
        Route="TextPage" />

</Shell>
